    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/node": "^20.10.5",
    "chai": "^4.3.10",
    "mitata": "^0.1.6",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.2"
  }
